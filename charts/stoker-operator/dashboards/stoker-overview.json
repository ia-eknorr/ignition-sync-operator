{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Fleet Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            { "options": { "0": { "text": "Not Ready" } }, "type": "value" },
            { "options": { "1": { "text": "Ready" } }, "type": "value" }
          ]
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": { "colorMode": "background", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "stoker_controller_cr_ready{namespace=~\"$namespace\", name=~\"$cr\"}",
          "legendFormat": "{{ name }}"
        }
      ],
      "title": "CR Ready",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "blue", "value": null }] } } },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "stoker_controller_gateways_discovered{namespace=~\"$namespace\", name=~\"$cr\"}",
          "legendFormat": "{{ name }}"
        }
      ],
      "title": "Gateways Discovered",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "green", "value": null }] } } },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "stoker_controller_gateways_synced{namespace=~\"$namespace\", name=~\"$cr\"}",
          "legendFormat": "{{ name }}"
        }
      ],
      "title": "Gateways Synced",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "purple", "value": null }] } } },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(rate(stoker_controller_reconcile_total{namespace=~\"$namespace\", name=~\"$cr\"}[5m]))",
          "legendFormat": "reconciles/s"
        }
      ],
      "title": "Reconcile Rate",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "title": "Controller Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 6 },
      "id": 5,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(stoker_controller_reconcile_duration_seconds_bucket{namespace=~\"$namespace\", name=~\"$cr\"}[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(stoker_controller_reconcile_duration_seconds_bucket{namespace=~\"$namespace\", name=~\"$cr\"}[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(stoker_controller_reconcile_duration_seconds_bucket{namespace=~\"$namespace\", name=~\"$cr\"}[5m]))",
          "legendFormat": "p99"
        }
      ],
      "title": "Reconcile Duration",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 6 },
      "id": 6,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(stoker_controller_ref_resolve_duration_seconds_bucket{namespace=~\"$namespace\", name=~\"$cr\"}[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(stoker_controller_ref_resolve_duration_seconds_bucket{namespace=~\"$namespace\", name=~\"$cr\"}[5m]))",
          "legendFormat": "p95"
        }
      ],
      "title": "Ref Resolution Duration",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 6 },
      "id": 7,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (result) (rate(stoker_controller_reconcile_total{namespace=~\"$namespace\", name=~\"$cr\"}[5m]))",
          "legendFormat": "{{ result }}"
        }
      ],
      "title": "Reconcile Results Rate",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 102,
      "title": "Agent Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 15 },
      "id": 8,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(stoker_agent_sync_duration_seconds_bucket{profile=~\"$profile\"}[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(stoker_agent_sync_duration_seconds_bucket{profile=~\"$profile\"}[5m]))",
          "legendFormat": "p95"
        }
      ],
      "title": "Agent Sync Duration",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 15 },
      "id": 9,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(stoker_agent_git_fetch_duration_seconds_bucket[5m]))",
          "legendFormat": "p50 {{ operation }}"
        },
        {
          "expr": "histogram_quantile(0.95, rate(stoker_agent_git_fetch_duration_seconds_bucket[5m]))",
          "legendFormat": "p95 {{ operation }}"
        }
      ],
      "title": "Git Fetch Duration",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 15 },
      "id": 10,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "stoker_agent_files_changed{profile=~\"$profile\"}",
          "legendFormat": "{{ profile }}"
        }
      ],
      "title": "Files Changed (Last Sync)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 23 },
      "id": 11,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(stoker_agent_scan_duration_seconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(stoker_agent_scan_duration_seconds_bucket[5m]))",
          "legendFormat": "p95"
        }
      ],
      "title": "Ignition Scan Duration",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 23 },
      "id": 12,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (result) (rate(stoker_agent_sync_total{profile=~\"$profile\"}[5m]))",
          "legendFormat": "{{ result }}"
        }
      ],
      "title": "Agent Sync Results Rate",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "id": 103,
      "title": "Webhooks",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
      "id": 13,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (source, status_code) (rate(stoker_webhook_receiver_requests_total[5m]))",
          "legendFormat": "{{ source }} ({{ status_code }})"
        }
      ],
      "title": "Webhook Requests Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "id": 14,
      "options": { "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (namespace, result) (rate(stoker_webhook_injector_injections_total[5m]))",
          "legendFormat": "{{ namespace }} ({{ result }})"
        }
      ],
      "title": "Sidecar Injections Rate",
      "type": "timeseries"
    }
  ],
  "schemaVersion": 39,
  "tags": ["stoker", "kubernetes", "ignition"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "definition": "label_values(stoker_controller_cr_ready, namespace)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "namespace",
        "query": "label_values(stoker_controller_cr_ready, namespace)",
        "refresh": 2,
        "type": "query"
      },
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "definition": "label_values(stoker_controller_cr_ready{namespace=~\"$namespace\"}, name)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "cr",
        "query": "label_values(stoker_controller_cr_ready{namespace=~\"$namespace\"}, name)",
        "refresh": 2,
        "type": "query"
      },
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "definition": "label_values(stoker_agent_sync_total, profile)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "profile",
        "query": "label_values(stoker_agent_sync_total, profile)",
        "refresh": 2,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Stoker Overview",
  "uid": "stoker-overview"
}
